<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Order.
    -->
    <changeSet id="20210410221713-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="jhi_order"
                                 constraintName="fk_jhi_order_client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="delivery_id"
                                 baseTableName="jhi_order"
                                 constraintName="fk_jhi_order_delivery_id"
                                 referencedColumnNames="id"
                                 referencedTableName="delivery"/>

    </changeSet>
</databaseChangeLog>
